{
  "name": "AI Agent + HTTP Tool Keypoint Extractor",
  "nodes": [
    {
      "parameters": {},
      "id": "manualTrigger",
      "name": "Start",
      "type": "n8n-nodes-base.manualTrigger",
      "position": [200, 240]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "chunk",
              "value": "{\"id\":\"1\",\"text\":\"Paste your chunk here\"}"
            },
            {
              "name": "agentPrompt",
              "value": "Extract key points from the text and return ONLY JSON array of {\"key\":\"...\",\"value\":\"...\"}"
            }
          ]
        }
      },
      "id": "setInput",
      "name": "Set Input",
      "type": "n8n-nodes-base.set",
      "position": [480, 240]
    },
    {
      "parameters": {
        "url": "http://192.168.28.137:11458/v1/generate",
        "method": "POST",
        "jsonParameters": true,
        "responseFormat": "json",
        "bodyParametersJson": "={ { \"prompt\": $json.prompt, \"max_tokens\": 600, \"temperature\": 0 } }"
      },
      "id": "httpLLM",
      "name": "LLM HTTP Tool",
      "type": "n8n-nodes-base.httpRequest",
      "position": [840, 420]
    },
    {
      "parameters": {
        "agentPrompt": "={{$json[\"agentPrompt\"]}}",
        "conversation": false,
        "additionalFields": {}
      },
      "id": "aiAgent",
      "name": "AI Agent",
      "type": "n8n-nodes-base.aiAgent",
      "typeVersion": 1,
      "position": [840, 240]
    },
    {
      "parameters": {
        "functionCode": "return [{ json: { prompt: `${$node[\"aiAgent\"].json.agentOutput}` } }];"
      },
      "id": "preparePrompt",
      "name": "Prepare Prompt for LLM",
      "type": "n8n-nodes-base.function",
      "position": [620, 420]
    },
    {
      "parameters": {
        "functionCode": "let txt = JSON.stringify($json);\nlet extracted;\ntry {\n  const m = txt.match(/(\\{[\\s\\S]*\\}|\\[[\\s\\S]*\\])/);\n  extracted = JSON.parse(m ? m[1] : txt);\n} catch(e) {\n  extracted = { error: e.message, raw: txt };\n}\nreturn [{ json: extracted }];"
      },
      "id": "parseResponse",
      "name": "Parse LLM Output",
      "type": "n8n-nodes-base.function",
      "position": [1160, 420]
    }
  ],
  "connections": {
    "manualTrigger": {
      "main": [[{ "node": "setInput", "type": "main" }]]
    },
    "setInput": {
      "main": [[{ "node": "aiAgent", "type": "main" }]]
    },
    "aiAgent": {
      "tool": [[{ "node": "preparePrompt", "type": "main" }]]
    },
    "preparePrompt": {
      "main": [[{ "node": "httpLLM", "type": "main" }]]
    },
    "httpLLM": {
      "main": [[{ "node": "parseResponse", "type": "main" }]]
    }
  }
}
